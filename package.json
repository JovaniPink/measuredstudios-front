{
  "name": "awesomenbadata.page",
  "description": "The frontend for @JovaniPink posts & ideas on NBA data, stats, and visualizations.",
  "version": "0.1.0",
  "private": true,
  "author": "",
  "license": "MIT",
  "keywords": [
    "react",
    "gatsby",
    "gatsby-starter",
    "typescript",
    "Tailwindcss"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check"
    }
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "npm lint --fix",
      "npm format"
    ]
  },
  "dependencies": {
    "gatsby": "2.32.4",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-icons": "^3.11.0",
    "framer-motion": "^2.9.5",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "tailwindcss": "^2.0.1",
    "twin.macro": "^2.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@testing-library/react-hooks": "3.7.0",
    "@testing-library/user-event": "12.7.2",
    "@types/jest": "25.2.3",
    "@types/node": "14.14.20",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "babel-jest": "25.5.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@emotion/babel-plugin-jsx-pragmatic": "^0.1.5",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-react": "7.22.0",
    "husky": "4.3.8",
    "identity-obj-proxy": "3.0.0",
    "jest": "25.5.4",
    "jest-watch-typeahead": "0.5.0",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "typescript": "3.9.7",
    "ts-jest": "25.5.1"
  },
  "babelMacros": {
    "twin": {
      "preset": "emotion"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">= 14.15.1"
  }
}
